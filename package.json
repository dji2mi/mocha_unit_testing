{
  "name": "mocha-unit-testing",
  "version": "1.0.0",
  "description": "unit test using mocha",
  "main": "Calculator.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "mocha": "^10.2.0",
    "mochawesome": "^4.0.1"
  },
  "scripts": {
    "test": "mocha --config ./test/config/mocha.config.js",
    "coverage": "nyc mocha --config ./test/config/mocha.config.js"
  },
  "nyc": {
    "all": true,
    "include": [
      "app/**/*.js"
    ],
    "excludes": [
      "**/*.spec.js"
    ],
    "reporter": [
      "html",
      "text"
    ]
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Anton Zimin",
  "license": "",
  "bugs": {
    "url": "https://github.com/dji2mi/mocha_unit_testing.git"
  },
  "homepage": "https://github.com/",
  "devDependencies": {
    "nyc": "^14.1.1"
  }
}
